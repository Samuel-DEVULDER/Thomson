***************************************
* Particules colorees explosantes.
*
* Assemblage sous MASS6809 (prehisto)
* avec l'option: A/IM
*
* Samuel Devulder, Mai 2011
***************************************
	org	$a000
	
NPARTS	set	65
MAXGRO	set	8

ini	jsr	inibas
	swi
	
inibas	pshs	d,x,y,u,dp,cc

	orcc	#$50
	jsr	init_ram
*	jsr	init_ecran
	jsr	init_conv
	jsr	init_vid
	jsr	init_son
	jsr	init_rnd
	jsr	init_part

boucle	
	jsr	anim
	jsr	$E81B
	bcc	boucle
	jsr	exit_son
	puls	d,x,y,u,dp,cc,pc
*	swi
***************************************
* initialisation du son
***************************************
init_son
	pshs	a,dp,x
	lda	#exit_son<-8
	tfr	a,dp
	setdp	exit_son<-8
	ldx	#$E7CF
	lda	-1,x
	sta	<MCRA+1
	lda	,x
	sta	<MCRB+1
	ora	#4
	sta	,x
	lda	-2,x
	sta	<MDB+1
	lda	,x
	anda	#$FB
	sta	,x
	lda	-2,x
	sta	<MDDB+1
	lda	#$3F
	sta	-2,x
	lda	#4
	sta	-1,x
	sta	,x
	clr	-2,x
	lda	-14,x
	sta	<MPCR+1
	ora	#$30
	anda	#$F7
	sta	-14,x
	setdp	0
	puls	a,dp,x,pc
***************************************
* sortie du son
***************************************
exit_son
	ldx	#$E7CF
	lda	,x
	anda	#$FB
	sta	,x
MDDB	lda	#0
	sta	-2,x
	lda	,x
	ora	#4
	sta	,x
MDB	lda	#0
	sta	-2,x
MCRB	lda	#0
	sta	,x
MCRA	lda	#0
	sta	-1,x
MPCR	lda	#0
	sta	-14,x
	rts
***************************************
* initialisation des particules
***************************************
init_part
	lda	#NPARTS
	ldb	#48
	ldu	#partic
ini_p0
	pshs	d
	jsr	new_part
	puls	d
	leau	8,u
	deca
	decb
	bne	ini_p0
	sta	nparts
	rts
***************************************
* prépare les echantillons sonores
***************************************
sound	ldx	#mute_sound
	ldu	#ecranR
	ldb	#50*3
	ldy	#sound3
sound0
*	lda	#$F1
	lda	,x+
	bne	sound1
	lda	,--x
sound1	sta	sound2+1
	lsra
	lsra
	lsra
	lsra
	lda	a,y
	sta	1,u
sound2	lda	#0
	anda	#15
	lda	a,y
	sta	,u
	leau	42,u
	decb
	bne	sound0
	stx	sound+1
	lda	ecranR
	ora	#128
	sta	ecranR
	lda	ecranV
	ora	#128
	sta	ecranV
	lda	ecranB
	ora	#128
	sta	ecranB
sound3	rts
	fcb	0,16,24,26,28,29,30
	fcb	31
	fcb	32,33,34,36,38,46,62
***************************************
* fait evoluer toutes les particules
***************************************
anim	bsr	sound
	ldu	#partic
anim1	jsr	anim_part
	cmpu	#partic+8*(NPARTS+1)
	bne	anim1
	ldx	#ecranB+42*50
	ldu	#$5F40-40
	bsr	update
	ldu	#$5F40-80
	bsr	update
	ldu	#$5F40-120
* Economie d'un saut
*	bsr	update
*	rts
***************************************
* Mise a jour de l'ecran
***************************************
updat	macro
	ldd	-2*(\0+1),x
	std	-2*(\0+1),u
	beq	updat\0
	lda	a,y
	ldb	b,y
	std	-2*(\0+1),x
updat\0	set	*
	endm
	
update	ldy	#convTB	
up0
	updat	0
	updat	1
	updat	2
	updat	3
	updat	4
	updat	5
	updat	6
	updat	7
	updat	8
	updat	9
* son
*	clr	$E7CD
	lda	-41,x
	sta	$E7CD
	updat	10
	updat	11
	updat	12
	updat	13
	updat	14
	updat	15
	updat	16
	updat	17
	updat	18
	updat	19
	leau	-4*40,u
	leax	-42,x
	lda	,x
	sta	$E7CD
	lbge	up0
	rts

***************************************
* Affiche une petite particule a une
* coordonee paire:
*       XX ..
*       XX ..
***************************************
drSmallPart0
	lda	#$77
	sta	-42,x
	sta	,x
	rts
***************************************
* Affiche une petite particule a une
* coordonee impaire:
*       .X X.
*       .X X.
***************************************
drSmallPart1
	stb	,-s
	ldd	-42,x
	ora	#$07
	orb	#$70
	std	-42,x
	ldd	,x
	ora	#$07
	orb	#$70
	std	,x
	puls	b,pc
***************************************
* Affiche une grosse particule a une
* coordonee paire:
*       .X X. ..
*       XX XX ..
*       XX XX ..
*       .X X. ..
***************************************
drBigPart0
	pshs	b
	ldd	#$7777
	std	,x
	std	42,x
	ldd	-42,x
	ora	#$07
	orb	#$70
	std	-42,x
	ldd	84,x
	ora	#$07
	orb	#$70
	std	84,x
	puls	b,pc
***************************************
* Affiche une grosse particule a une
* coordonee impaire:
*       .. XX ..
*       .X XX X.
*       .X XX X.
*       .. XX ..
***************************************
drBigPart1
	pshs	b
	ldd	#$0777
	stb	-41,x
	stb	85,x
	ora	,x
	std	,x
	lda	#$07
	ora	42,x
	std	42,x
	lda	#$70
	ora	2,x
	sta	2,x
	lda	#$70
	ora	44,x
	sta	44,x
	puls	b,pc
***************************************
* initialisation du generateur rnd
***************************************
init_rnd
	lda	$E7E5
	sta	,-s
	bsr	rnd
	dec	,s
	puls	a,pc
***************************************
* Cree une nouvelle particule pointee
* par reg U.
***************************************
new_part
* duree de vie
	bsr	rnd
	andb	#31
	addb	#6+128
	stb	,u
* couleur
newp2	bsr	rnd
	andb	#7
	beq	newp2
	stb	1,u
* coordonees
	bsr	rnd
	lsrb
	lsrb
	lsrb
	lsrb
	andb	#7
	addb	#(80-7)/2
	lslb
	lda	#(50-4)*4
	std	2,u
* vitesse 
	bsr	rnd
	lda	#9
	mul
	suba	#4
	pshs	a
	bsr	rnd
	tfr	b,a
	anda	#15
	adda	#10
	nega
	puls	b
	std	4,u
	ldd	#256
	std	6,u
* sifflement
	ldd	#fsss_sound
	std	sound+1
	rts
***************************************
* Genere un nombre pseudo aleatoire sur
* 8 bit. C'est une version modifiee
* du multiply with carry. La periode
* de la sequence est 31870. Compact
* et rapide, que demander de plus?
***************************************
rnd	ldd	#3*256+249
	mul
rnd1	addd	#0
	sta	rnd1+2	!,pcr
	stb	rnd+1	!,pcr
	rts
***************************************
* Fait evoluer la particule pointee par
* reg U.
***************************************
anim_part
	ldb	,u	* vivante?
	beq	exit_part
	decb		* vieillesse
	bvs	boom_part
	bne	affi_part
	
* Particule morte
mort_part
	clr	,u
	ldb	nparts
	incb
	stb	nparts
	beq	nouv_part
	cmpb	#NPARTS-MAXGRO
	blt	exit_part
* A faire: si nb grosse <4, alors new part, sinon rien
nouv_part
	decb
	stb	nparts
	bsr	new_part
* sortie animation
exit_part
	leau	8,u
	rts

* Affichage
affi_part
	stb	,u
* calcul pointeur video
	ldx	#ecranR+2
	ldd	2,u
	lsrb
	lsrb
	abx
	lsra
	lsra
	inca
	ldb	#42
	mul
	leax	d,x

* routine de dessin
	lda	,u
	ldb	3,u
	lsrb
	lsla
	rolb
	lslb
	andb	#6
	ldy	#anim_tab
	ldy	b,y

* affichage
	ldb	1,u
	lsrb
	bcc	*+4
	jsr	,y
	leax	42*50,x
	lsrb
	bcc	*+4
	jsr	,y
	leax	42*50,x
	lsrb
	bcc	*+4
	jsr	,y

* deplacement
	ldd	2,u	* y,x
	adda	4,u	* vy,vx
	addb	5,u
	cmpb	#(80-3)*2
	bhi	mort_part
	cmpa	#(50-4)*4
	bhi	mort_part
	std	2,u
	ldd	4,u
	addd	6,u	* fy,fx
	std	4,u
	bra	exit_part
***************************************
* explosion particule
***************************************
boom_part
	ldb	nparts
	cmpb	#(vit_tab2-vit_tab-2)/2
	blt	mort_part
* bruitage: boom
	ldd	#boom_sound
	std	sound+1
* duree vie des sous-particules
	jsr	rnd
	lsrb
	andb	#31
	addb	#10
	stb	,u
	ldy	#vit_tab
	ldx	#partic-8
boom0	ldd	,y++
	beq	exit_part
boom1	leax	8,x
	tst	,x
	bne	boom1
	dec	nparts
* copie particule mini vers la nouvelle
	addd	4,u
	std	4,x
	ldd	,u
	std	,x
	ldd	2,u
	std	2,x
	ldd	6,u
	std	6,x
	bra	boom0
***************************************
* table des routines d'affichage
***************************************	
anim_tab
	fdb	drSmallPart0
	fdb	drBigPart0
	fdb	drSmallPart1
	fdb	drBigPart1
***************************************
* Table des vitesses des sous-
* particules
***************************************
vit_tab		* vy,vx
***************************************
*       |
*     - * -
*       |
***************************************
        fcb	0,2
	fcb	0,-2
	fcb	4,0
	fcb	-4,0
***************************************
*      \ /
*       * 
*      / \
***************************************	
	fcb	4,1
	fcb	4,-1
	fcb	-4,1
	fcb	-4,-1
***************************************
*     .   .
*       * 
*     `   '
***************************************
	fcb	2,2
	fcb	2,-2
	fcb	-2,2
	fcb	-2,-2
***************************************
*      . .
*       * 
*      ` '
***************************************
	fcb	2,1
	fcb	2,-1
	fcb	-2,1
	fcb	-2,-1
***************************************
*       
*       *
*       
***************************************
	fcb	0,0
