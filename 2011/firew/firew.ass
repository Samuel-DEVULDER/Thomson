***************************************
* Initialisation ecran. Tour en noir et
* couleur de fond alternant R,V,B,Noir
***************************************
init_vid
	lda	#$E7
	setdp	$E7
	tfr	a,dp
	ldb	<$E7C3	* forme
*	orb	#1
	andb	#254	
	stb	<$E7C3
