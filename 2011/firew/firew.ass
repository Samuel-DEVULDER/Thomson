	ldu	#$5F40
	bsr	fillscr
	inc	<$E7C3
	clrb
	bsr	fillscr
	dec	<$E7C3	* fait au dessus
	ldd	#-40*3*256+7*8+64+128
	bsr	fillscr
	leau	-40,u
	ldb	#8+64+128
	bsr	fillscr
	leau	-40,u
	ldb	#16+64+128
	bsr	fillscr
	leau	-40,u
	ldb	#32+64+128
	bsr	fillscr
	inc	<$E7C3
*	ldb	<$E7DD
*	andb	#240
*	stb	<$E7DD
	setdp	$0
* tour noir de façon portable
*	ldb	#27
*	jsr	$E803
*	ldb	#$60
*	jmp	$E803
	rts
***************************************
* Effacement ram
***************************************
init_ram
	ldx	#debram
initr1	clr	,x+
	cmpx	#finram+1
	bne	initr1
	rts
***************************************
* Initialisation des sentinelles 
* de la ram ecran
***************************************
*init_ecran
*	ldx	#ecranR
*	lda	#-1
*init_ecran0
*	sta	,x
*	leax	42,x
*	cmpx	#ecranB+42*50
*	bne	init_ecran0
*	deca
*	sta	ecranR
*	sta	ecranV
*	sta	ecranB
*	rts
***************************************
* Remplissage de l'ecran avec la valeur
* contendue dans le registre B. A
* chaque fin de ligne l'adresse video
* est ajoutée à la valeur du registre A
* permettant de faire des remplissages
* de lignes non consecutives.
***************************************
fillscr
	pshs	d,x,y,u
	sts	fill1-4
	sta	fill2+2
	tfr	b,a
	tfr	d,x
	leay	,x
	leas	,x
fill0	pshu	d,x,y,s
	cmpa	,u
	beq	fill1
	lds	#$1234
	puls	d,x,y,u,pc
fill1	pshu	d,x,y,s
	pshu	d,x,y,s
	pshu	d,x,y,s
	pshu	d,x,y,s
fill2	leau	$55,u
	bra	fill0
***************************************
* Initialisation de la table de
* conversion. 
***************************************	
init_conv
	ldu	#convTB
	clra
	bsr	initc1
	leau	32,u
	clra
	bsr	initc1
	leau	64,u
	lda	#32
	bsr	initc1
	leau	16,u
	lda	#96
initc1	sta	,u
	sta	2,u
	ora	#2
	sta	6,u
	ora	#4
	sta	7,u
	rts
***************************************
* Echantillons sonore
***************************************
mute_sound
	fdb	$8800
boom_sound
	includ	BOOM.ASM
	fcb	$00
fsss_sound
	includ	FSSS.ASM
	fcb	$00
***************************************
* debut zone donnees	
***************************************
debram	set	*
***************************************
* Tableau de conversion. Pour chaque
* couple de valeurs il donne le couple
* precedent.
***************************************
convTB	rmb	7*17+1
***************************************
* Ecran rouge, vert, bleu
***************************************
ecranR	rmb	42*50
ecranV	rmb	42*50
ecranB	rmb	42*50
***************************************
* Nombre de particules libres
***************************************
nparts	rmb	1
***************************************
* Particules
***************************************
* vie+type,couleur,x,y,vx,vy,fx,fy
partic	rmb	8*NPARTS
finram	set	*	
***************************************
* Fin du programme: on demarre sur 
* le point d'entree "ini".
***************************************
