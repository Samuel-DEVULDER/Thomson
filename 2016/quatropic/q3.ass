(main)Q3
        org     $A000
        setdp   *<-8

KTST    equ     $E809
        
main    pshs    d,x,y,u,dp,cc


        lda     $E7CD
        ora     #$3F
        sta     $E7CD
                
wait    jsr     KTST
        bcs     wait

        lda     #*<-8
        tfr     a,dp

        ldx     #zik

* speed -> duree ligne
        lda     ,x+
		sta		<speed

* lecture sequence
rdseq   ldu     ,x++
        bne     newptn
        ldx     #zikbak
        bra     rdseq

* lecture d'une ligne pattern
newptn  jsr     KTST        ; test clavier
        bcs     exit
        
        ldy		#$128b		; pas de bruit
		ldd		,u++
		beq		rdseq		; fin pattern?
		bitb	#1			
		beq		initpt0
		ldy		#$4889		; bruit
initpt0	sty		<noise
      
		ldb		#0
speed	set		*-1
		mul
		tfr		d,y			; duree pattern
	  
        ldd     ,u++
        sta     <ch1rat
        stb     <ch2rat
        ldd     ,u++
        sta     <ch3rat
        stb     <ch4rat
        
        ldd     ,u++
        std     <ch1frq
        ldd     ,u++
        std     <ch2frq
        ldd     ,u++
        std     <ch3frq
        ldd     ,u++
        std     <ch4frq

        bra    playptn
        
exit    
        puls    d,x,y,u,dp,cc,pc

* joue un pattern Y=durée
playptn pshs    cc,d,x,y,u
        orcc    #$50
        sts     <savsp

        ldx     #0
		leas	,x
		leau	,x
		stx		<noise-2
        		
playlp  set     *
		ldd		#$3300+$800	; 3
		adcb	#0		; 2
inib	set		*-1
		lslb			; 2
		lslb			; 2
		lslb			; 2
		lslb			; 2
		addb	#-16	; 2
		bcc		iniz	; 3
		eora	#8		; 2		
		bra		iniz	; 3
iniz	sta		$E7C1	; 5 => 3+2*6+3+5(+5)=23(+5)
		eora	#8		; 2		

* oscillo 1 : 8+4+2+3(+5)=>17
        leax    $8000,x ; 8
ch1frq  set     *-2     ; freq
        cmpx    #$8000  ; 4
ch1rat  set     *-2
        adcb    #-16    ; 2 
		bcc		*+5		; 3
		sta		$E7C1   ; 5
        
* oscillo 2 : 8+5+2+3(+5)=>18
        leau    $8000,u ; 8
ch2frq  set     *-2     ; freq
        cmpu    #$8000  ; 5
ch2rat  set     *-2
        adcb    #-16    ; 2 
		bcc		*+5		; 3
		sta		$E7C1	; 5   

* oscillo 3 : 8+5+2+3(+5)=>18
        leas    $8000,s ; 8
ch3frq  set     *-2     ; freq
        cmps    #$8000  ; 5
ch3rat  set     *-2
        adcb    #-16    ; 2 
		bcc		*+5		; 3
		sta		$E7C1   ; 5
		
* oscillo 4 / noise : 4+3+2+2+4+5+2=22
		stb		<inib	; 4
        ldd     #0      ; 3
noise   set     *
        lsla            ; 2
        adca    #0      ; 2
*       nop
*       adda    #0
        addd    #0      ; 4
ch4frq  set     *-2     ; freq
        std     <noise-2    ; 5
        cmpa    #$80    ; 2 (rap. cyclique)
ch4rat	set		*-1

* bouclage
        leay    -1,y    ; 4
        bne     playlp  ; 3=>23+17+18+18+22+7(+5)=110
        
        lds     #0
savsp   set     *-2
        puls    cc,d,x,y,u
		jmp		<newptn

        includ  ZIK2
        
        end     main
