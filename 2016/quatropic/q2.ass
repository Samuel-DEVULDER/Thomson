(main)QUATTRPC
        org     $A000
        setdp   *<-8

KTST    equ     $E809
        
main    pshs    d,x,y,u,dp,cc

        lda     $E7CD
        ora     #$3F
        sta     $E7CD
        
wait    jsr     KTST
        bcs     wait

        lda     #*<-8
        tfr     a,dp

        ldx     #zik

* speed -> duree ligne
        lda     ,x+
		sta		<speed

* lecture sequence
rdseq   ldu     ,x++
        bne     newptn
        ldx     #zikbak
        bra     rdseq

* lecture d'une ligne pattern
newptn  jsr     KTST        ; test clavier
        bcs     exit

		ldy		#$128b		; pas de bruit
		ldd		,u++
		beq		rdseq		; fin pattern?
		bitb	#1			
		beq		initpt0
		ldy		#$4889		; bruit
initpt0	sty		<noise
      
		ldb		#0
speed	set		*-1
		mul
		tfr		d,y			; duree pattern
	  
        ldd     ,u++
        sta     <ch1rat
        stb     <ch2rat
        ldd     ,u++
        sta     <ch3rat
        stb     <ch4rat
        
        ldd     ,u++
        std     <ch1frq
		bne		sp1
		std		<ch1frq-3
sp1     ldd     ,u++
        std     <ch2frq
   		bne		sp2
		std		<ch2frq-3
sp2     ldd     ,u++
        std     <ch3frq
   		bne		sp3
		std		<ch3frq-3
sp3     ldd     ,u++
        std     <ch4frq
		bne		sp4
		std		<noise-2
sp4

        bra    playptn

exit    puls    d,x,y,u,dp,cc,pc

* joue un pattern Y=durée
playptn pshs    cc,d,x,y,u
        orcc    #$50
        sts     <savsp
        ldx     #$E7C0
        ldd     ,x
        orb     #8
        tfr     d,s     ; x=buzzer 0
        eorb    #8
        tfr     d,u     ; u=buzzer 1

playlp  set     *

* oscillo 1
        ldd     #0      ; 3
        addd    #0      ; 4
ch1frq  set     *-2     ; freq
        std     <*-5    ; 5
        cmpa    #$80    ; 2 (rap. cyclique)
ch1rat  set     *-1
        blo     ch1zer  ; 3
		fcb		$10
ch1zer  stu     ,x      ; 5 ; 3+4+5+2+3+5=>22

* oscillo 2
        ldd     #0      ; 3
        addd    #0      ; 4
ch2frq  set     *-2     ; freq
        std     <*-5    ; 5
        cmpa    #$80    ; 2 (rap. cyclique)
ch2rat  set     *-1
        bhi     ch2zer  ; 3
		fcb		$10
ch2zer  stu     ,x      ; 5 ; 3+4+5+2+3+5=>22

* oscillo 4 / noise
        ldd     #0      ; 3
noise   set     *
        lsla            ; 2
        adca    #0      ; 2
*       nop
*       adda    #0
        addd    #0      ; 4
ch4frq  set     *-2     ; freq
        std     <noise-2 ; 5
        cmpa    #$80    ; 2 (rap. cyclique)
ch4rat  set     *-1
        blo     ch4zer  ; 3
		fcb		$10
ch4zer  stu     ,x      ; 5

* oscillo 3
        ldd     #0      ; 3
        addd    #0      ; 4
ch3frq  set     *-2     ; freq
        std     <*-5    ; 5
        cmpa    #$80    ; 2 (rap. cyclique)
ch3rat  set     *-1
        bhi     ch3zer  ; 3
		fcb		$10
ch3zer  stu     ,x      ; 5

* bouclage
        leay    -1,y    ; 4
        bne     playlp  ; 3=>111
        
        lds     #0
savsp   set     *-2
        puls    cc,d,x,y,u
		jmp		<newptn

        includ  ZIK
        
        end     main
